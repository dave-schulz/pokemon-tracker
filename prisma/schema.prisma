generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        Int      @id @default(autoincrement())
  store     String
  title     String
  link      String   @unique
  price     String
  oldPrice  String?
  inStock   Boolean? @default(true)
  lastSeen  DateTime @default(now())
  createdAt DateTime @default(now())
}
