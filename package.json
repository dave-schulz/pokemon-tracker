{
  "name": "pokemon-tracker",
  "version": "1.0.0",
  "description": "Scraper die Pok√©mon producten van Bol.com monitort.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && pnpm exec playwright install --with-deps",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "lint:fix": "eslint 'src/**/*.{ts,js}' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "scrape": "tsx src/index.ts"
  },
  "keywords": [
    "scraper",
    "pokemon",
    "bol.com",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.18.3",
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.9.3"
  }
}
